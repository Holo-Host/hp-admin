    "webpack-dev-server": "3.2.1",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "4.2.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "global": [
      "afterAll",
      "afterEach",
      "beforeAll",
      "beforeEach",
      "describe",
      "expect",
      "it",
      "jest",
      "test"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "react-css-modules",
        {
          "generateScopedName": "[name]__[local]___[hash:base64:5]",
          "exclude": "node_modules"
        }
      ],
      [
        "module-resolver",
        {
          "root": [
            "src"
          ],
          "extensions": [
            ".graphql"
          ]
        }
      ],
      "import-graphql"
    ]
  }
}
