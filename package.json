    "@babel/core": "7.4.3",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@svgr/webpack": "4.1.0",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^9.1.3",
    "@testing-library/react-hooks": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "1.6.0",
